    private void jButton16ActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        DefaultTableModel linha = (DefaultTableModel) tabelaAtributo.getModel();
        //String valore = (String) tabelaAtributo.getValueAt(0,0);//pega valor de campo(coluna, linha)
        //System.out.print("auqi"+valore);
        int row = linha.getRowCount();
        int col = linha.getColumnCount();
        Object[][] dados = new Object[row][col];

        for(int i=0;i<row;i++){
            for(int j=0;j<col;j++){
                dados[i][j] = linha.getValueAt(i, j);
                System.out.print("IJ"+i+j+":"+dados[i][j]);
            }
            System.out.println("");
        }//fecha for
        
        //Atributo atrib = new Atributo(dados);
        //montar json
        //cria um JSONArray e preenche com valores string
        JSONObject jsonAtributo = new JSONObject();
        JSONArray objArrayAtributo = new JSONArray();
        //JSONObject objAtributo = new JSONObject();
        for(int i=0;i<row;i++){
            JSONObject objAtributo = new JSONObject();
            try {
                objAtributo.put("nomeAtributo", dados[i][0]);
            } catch (JSONException ex) {
                Logger.getLogger(CadModClasse.class.getName()).log(Level.SEVERE, null, ex);
            }
            try {
                objAtributo.put("tipoAtributo", dados[i][1]);
            } catch (JSONException ex) {
                Logger.getLogger(CadModClasse.class.getName()).log(Level.SEVERE, null, ex);
            }
            try {
                objAtributo.put("valor", dados[i][2]);
            } catch (JSONException ex) {
                Logger.getLogger(CadModClasse.class.getName()).log(Level.SEVERE, null, ex);
            }
            try {
                objAtributo.put("qualificador", dados[i][3]);
            } catch (JSONException ex) {
                Logger.getLogger(CadModClasse.class.getName()).log(Level.SEVERE, null, ex);
            }
               
           
            System.out.println("");
      
        try {
                  //insere o array no JSONObject com o rótulo "atributo"
                  jsonAtributo.put("atributo", objArrayAtributo);
              } catch (JSONException ex) {
                  Logger.getLogger(CadModClasse.class.getName()).log(Level.SEVERE, null, ex);
              } 
        objArrayAtributo.put(objAtributo);
        String json_string = jsonAtributo.toString();
        System.out.println(json_string);
    }                                         

